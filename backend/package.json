{
  "name": "backend",
  "version": "1.0.0",
  "description": "Estrutura base para API REST com Express.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "npm run test:prepare && jest --coverage",
    "test:unit": "npm run test:prepare && jest --coverage --runTestsByPath src/tests/unit",
    "test:integration": "npm run test:prepare && jest --coverage --runTestsByPath src/tests/integration",
    "test:watch": "npm run test:prepare && jest --watch",
    "test:prepare": "cross-env TEST_DATABASE_URL=\"file:./dev-test.db\" prisma generate --schema=prisma/schema.test.prisma && cross-env TEST_DATABASE_URL=\"file:./dev-test.db\" prisma db push --schema=prisma/schema.test.prisma && node -e \"console.log('Test client ready')\"",
    "test:clear-cache": "jest --clearCache",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset --skip-seed=false",
    "prisma:seed": "prisma db seed",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "overrides": {
    "@prisma/engines-version": "*"
  }
}
